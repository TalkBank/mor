@UTF8
%********
%* NOUNS
%********

RULENAME: n:let
% these get [allo n0]
LEX-ENTRY:
LEXCAT = [scat n:let]
LEXSURF = $X
ALLO:
ALLOSURF = LEXSURF
ALLOCAT = LEXCAT, ADD [allo n0], ADD [block let]

RULENAME: n-plurale-tantum
% don't change plurale tantum nouns
LEX-ENTRY:
LEXCAT = [scat n:pt]
LEXSURF = $X
ALLO:
ALLOSURF = LEXSURF
ALLOCAT = LEXCAT, ADD [allo n0]
ALLO:
ALLOSURF = $X_$X
ALLOCAT = LEXCAT, ADD [allo n0], ADD [gen DUP]

RULENAME: n-um
LEX-ENTRY:
LEXCAT = [scat n], [pl a]
LEXSURF = $Xum
ALLO:
ALLOSURF = LEXSURF
ALLOCAT = LEXCAT, ADD [allo n0]
ALLO:
ALLOSURF = $Xa
ALLOCAT = LEXCAT, ADD [allo n0], ADD [gen PL]

RULENAME: n-us
LEX-ENTRY:
LEXCAT = [scat n], [pl i]
LEXSURF = $Xus
ALLO:
ALLOSURF = LEXSURF
ALLOCAT = LEXCAT, ADD [allo n0]
ALLO:
ALLOSURF = $Xi
ALLOCAT = LEXCAT, ADD [allo n0], ADD [gen PL]

RULENAME: n-man
LEX-ENTRY:
LEXSURF = $Xman
LEXCAT = [scat n], ![men no]
ALLO:
ALLOSURF = $Xmen
ALLOCAT = LEXCAT, ADD [num pl], ADD [allo nM]
ALLOSTEM = $Xman&PL
ALLO:
ALLOSURF = LEXSURF
ALLOCAT = LEXCAT, ADD [allo nM], ADD [pl -]
ALLO:
ALLOSURF = $Xman_$Xman
ALLOCAT = LEXCAT, ADD [allo n0], ADD [gen DUP]

RULENAME: n-chief
LEX-ENTRY:
LEXSURF = $Xf
LEXCAT = [scat n], [allo nV]
ALLO:
ALLOSURF = LEXSURF
ALLOCAT = LEXCAT, DEL [allo], ADD [allo n0]
ALLO:
ALLOSURF = $Xv
ALLOCAT = LEXCAT, DEL [allo], ADD [allo nVb]
ALLO:
ALLOSURF = $Xf_$Xf
ALLOCAT = LEXCAT, ADD [allo n0], ADD [gen DUP]

RULENAME: n-wife
LEX-ENTRY:
LEXSURF = $Xfe
LEXCAT = [scat n], [allo nV]
ALLO:
ALLOSURF = LEXSURF
ALLOCAT = LEXCAT, DELl [allo], ADD [allo n0]
ALLO:
ALLOSURF = $Xf
ALLOCAT = LEXCAT, DEL [allo], ADD [allo nVa]
ALLO:
ALLOSURF = $Xv
ALLOCAT = LEXCAT, DEL [allo], ADD [allo nVb]
ALLO:
ALLOSURF = $Xfe_$Xfe
ALLOCAT = LEXCAT, ADD [allo n0], ADD [gen DUP]

RULENAME: n-cry
LEX-ENTRY:
LEXSURF = $Yy
LEXCAT = [scat OR n det:num]
ALLO:
ALLOSURF = $Yi
ALLOCAT = LEXCAT, ADD [allo nYb], ADD [bare no]
ALLO:
ALLOSURF = $Y
ALLOCAT = LEXCAT, ADD [allo nYc], ADD [bare no]
ALLO:
ALLOSURF = LEXSURF
ALLOCAT = LEXCAT, ADD [allo nYa]
ALLO:
ALLOSURF = $Yy_$Yy
ALLOCAT = LEXCAT, ADD [allo n0], ADD [gen DUP]

